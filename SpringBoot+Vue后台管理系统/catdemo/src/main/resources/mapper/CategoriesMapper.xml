<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.catdemo.mapper.CategoriesMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.springboot.catdemo.entity.Categories">
        <id column="id" property="id" />
        <result column="menuName" property="menuName" />
        <result column="menuIconUrl" property="menuIconUrl" />
        <result column="type" property="type" />
        <result column="size" property="size" />
        <result column="md5" property="md5" />
        <result column="is_delete" property="isDelete" />
        <result column="enable" property="enable" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, menuName, menuIconUrl, type, size, md5, is_delete, enable
    </sql>

</mapper>
